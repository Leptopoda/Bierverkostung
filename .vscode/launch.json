  "version": "0.2.0",
  "configurations": [
    {
      "name": "bierverkostung",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define",
        "local_firebase=false",
      ],
    },
    {
      "name": "bierverkostung-local-android",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define",
        "local_firebase_ip=10.0.2.2",
      ],
    },
    {
      "name": "bierverkostung-attach",
      "request": "attach",
      "type": "dart",
    },
    {
      "name": "bierverkostung-profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "bierverkostung-release",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
    }
    /* {
      "type": "node",
      "request": "launch",
      "name": "Mocha Current File",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--no-timeouts",
